<Window x:Class="SuiBot_V2_Windows.Windows.Settings.Dialogs.EditListOfChannelsAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SuiBot_V2_Windows.Windows.Settings.Dialogs"
        mc:Ignorable="d"
        Title="Add Channel" Height="111.397" Width="310.294" ResizeMode="NoResize">
    <Grid>
        <Label Content="Channel" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="68,12,0,0" Text="{Binding NewChannel}" TextWrapping="Wrap" VerticalAlignment="Top" Width="226" />
        <Button x:Name="B_OK" Content="OK" HorizontalAlignment="Left" Margin="68,49,0,0" VerticalAlignment="Top" Width="75" Click="B_OKClicked"/>
        <Button x:Name="B_Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="148,49,0,0" VerticalAlignment="Top" Width="75" Click="B_CancelClicked"/>

    </Grid>
</Window>
