<Window x:Class="SuiBot_V2_Windows.Windows.EditChannel.Dialogs.AddEditUsernameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SuiBot_V2_Windows.Windows.EditChannel.Dialogs"
        mc:Ignorable="d"
        Title="AddEditUsernameDialog" Height="110" Width="360" ResizeMode="NoResize">
    <Grid>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="76,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="268" Text="{Binding ReturnedUsername}"/>
        <Button Content="OK" HorizontalAlignment="Center" Margin="0,0,80,10" VerticalAlignment="Bottom" Width="75" Click="Button_OKClicked"/>
        <Button Content="Cancel" HorizontalAlignment="Center" Margin="80,0,0,10" VerticalAlignment="Bottom" Width="75" Click="Button_CancelClicked"/>

    </Grid>
</Window>
